

<div class="container animated animated-fast fadeIn centrado-v-h" id="resumen_index">

    <div class="panel">

        <h4>Resumen mensual de consumos</h4>
        <br>

        <form class="form-horizontal" action="/quizes/resumen" method="post">

            <div class="form-group">
                <label for="mes" class="col-sm-2">Mes:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="mes" name="resumen[mes]" value="<%= resumen.mes %>" placeholder="mes"/>
                </div>
            </div>

            <div class="form-group">
                <label for="anio" class="col-sm-2">Año:</label>
                <div class="col-sm-10">
                    <input type="text" class="form-control" id="anio" name="resumen[anio]" value="<%= resumen.anio %>" placeholder="año"/>
                </div>
            </div>

            <div class="form-group text-center">

                    <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Ver</button>

            </div>



        </form>


    </div>



</div>
