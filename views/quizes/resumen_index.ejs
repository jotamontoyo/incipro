
    <h1>Resumen</h1>

    <form action="/quizes/resumen" method="post">

        <div class="form-group">
            <label for="mes" class="col-sm-2 control-label">Mes:</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="mes" name="resumen[mes]" value="<%= resumen.mes %>" placeholder="mes"/>
            </div>
        </div>

        <div class="form-group">
            <label for="any" class="col-sm-2 control-label">Año:</label>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="any" name="resumen[any]" value="<%= resumen.any %>" placeholder="año"/>
            </div>
        </div>

        <button type="submit" class="btn btn-success"><span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Solicitar</button>



    </form>
